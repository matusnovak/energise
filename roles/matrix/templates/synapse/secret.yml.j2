apiVersion: v1
kind: Secret
metadata:
  name: "{{ matrix.synapse.name }}"
  namespace: "{{ common.namespace }}"
  labels:
    {{ service.extraLabels | to_nice_yaml | indent(4) }}
type: "Opaque"
data:
  "{{ common.domain }}.signing.key": |
    {{ matrix.synapse.secrets.signingkey | b64encode | indent(4) }}
